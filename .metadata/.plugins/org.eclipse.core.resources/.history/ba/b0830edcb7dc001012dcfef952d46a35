package org.joonzis.DI_3;

public class UserTV {
	public static void main(String[] args) {
		LgTV ltv = new LgTV();
		ltv.powerOn();
		ltv.powerOff();
		ltv.volumeUp();
		ltv.volumeDown();
		
		System.out.println("-------------");

		SamsungTV stv = new SamsungTV();
		stv.powerOn();
		stv.powerOff();
		stv.volumeUp();
		stv.volumeDown();
	}

}
